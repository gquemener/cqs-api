{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "definitions": {},
    "id": "http://moov.one/program.json",
    "type": "object",
    "required": [ "id", "description" ],
    "additionalProperties": false,
    "properties": {
        "id": { "type": "string" },
        "name": { "type": "string" },
        "category": { "type": "string" },
        "type": { "type": "string" },
        "coaches": {
            "type": "array",
            "items": {
                "type": "object",
                "required": [ "id" ],
                "properties": {
                    "id": { "type": "string" }
                }
            }
        },
        "tracks": {
            "type": "array",
            "items": {
                "type": "object",
                "required": [ "id" ],
                "properties": {
                    "id": { "type": "string" }
                }
            }
        }
    }
}
